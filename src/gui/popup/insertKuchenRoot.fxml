<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="anchorPane" prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.popup.InsertKuchenController">
    <GridPane layoutX="14.0" layoutY="7.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <Label text="Hersteller" GridPane.rowIndex="0" />
        <Label text="Bodentyp" GridPane.rowIndex="1" />
        <Label text="Belag:" GridPane.rowIndex="2" />
        <Label text="Apfel" GridPane.rowIndex="3" />
        <Label text="Birne" GridPane.rowIndex="4" />
        <Label text="Kirsche" GridPane.rowIndex="5" />
        <Label text="Sahne" GridPane.rowIndex="6" />
        <Label text="Pudding" GridPane.rowIndex="7" />
        <ChoiceBox fx:id="herstellerNameChoiceBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="0" />
        <ChoiceBox fx:id="bodenTypChoiceBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Button mnemonicParsing="false" onAction="#reset" text="Zurücksetzen" GridPane.columnIndex="1" GridPane.rowIndex="9" />
        <Button mnemonicParsing="false" onAction="#confirm" text="Bestätigen" GridPane.rowIndex="9" />
        <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
            <Button mnemonicParsing="false" onAction="#apfelMinus" text="-" />
            <TextField fx:id="apfelTextField" editable="false" prefWidth="30.0" text="0" />
            <Button mnemonicParsing="false" onAction="#apfelPlus" text="+" />
        </HBox>
        <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
            <Button mnemonicParsing="false" onAction="#birneMinus" text="-" />
            <TextField fx:id="birneTextField" editable="false" prefWidth="30.0" text="0" />
            <Button mnemonicParsing="false" onAction="#birnePlus" text="+" />
        </HBox>
        <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="5">
            <Button mnemonicParsing="false" onAction="#kirscheMinus" text="-" />
            <TextField fx:id="kirscheTextField" editable="false" prefWidth="30.0" text="0" />
            <Button mnemonicParsing="false" onAction="#kirschePlus" text="+" />
        </HBox>
        <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="6">
            <Button mnemonicParsing="false" onAction="#sahneMinus" text="-" />
            <TextField fx:id="sahneTextField" editable="false" prefWidth="30.0" text="0" />
            <Button mnemonicParsing="false" onAction="#sahnePlus" text="+" />
        </HBox>
        <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="7">
            <Button mnemonicParsing="false" onAction="#puddingMinus" text="-" />
            <TextField fx:id="puddingTextField" editable="false" prefWidth="30.0" text="0" />
            <Button mnemonicParsing="false" onAction="#puddingPlus" text="+" />
        </HBox>
    </GridPane>
</AnchorPane>